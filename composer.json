{
    "name": "agnarodegard/isbn",
    "description": "A simple PHP ISBN handling library.",
    "keywords": [
        "isbn"
    ],
    "homepage": "https://github.com/agnarodegard/isbn",
    "license": "MIT",
    "authors": [
        {
            "name": "Agnar Ødegård",
            "email": "agnar@norweb.no",
            "homepage": "http://www.norweb.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Agnarodegard\\Isbn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agnarodegard\\Isbn\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
